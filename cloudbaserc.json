{
  "version": "1.0",
  "envId": "{{env.ENV_ID}}",
  "framework": {
    "name": "drawio",
    "plugins": {
      "client": {
        "use": "@cloudbase/framework-plugin-container",
        "inputs": {
          "serviceName": "drawio",
          "cpu": 2,
          "mem": 4,
          "servicePath": "/",
          "uploadType": "image",
          "imageInfo": {
            "imageUrl": "ccr.ccs.tencentyun.com/tcbimages/drawio-tcb:1.0"
          },
          "containerPort": 8080,
          "customLogs": "stdout,/dev/stdout,/dev/stderr"
        }
      }
    },
    "requirement": {
      "addons": []
    }
  }
}